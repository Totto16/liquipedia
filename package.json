{
    "name": "liquipedia-scripts",
    "version": "1.0.1",
    "description": "liquipedia scripts with npm and browserify for violentmonkey",
    "private": true,
    "scripts": {
        "compile": "tsc",
        "make": "npm run compile && browserify -p esmify --outfile ./liquipedia.user.js --entry ./temp/src/liquipedia.user.brows.js; rimraf ./temp/; rimraf liquipedia.user.js.tmp-*",
        "build": "npm run make",
        "dev": "nodemon --watch src/ -e .ts --exec \"npm run build || exit 1\"",
        "dev:s": "nodemon --watch * -e brows.js --exec \"npm run make:sto\"",
        "update": "npm update && npm-check -u --skip-unused",
        "prettier:debug": "npx prettier --check .",
        "prettier": "npx prettier --write .",
        "format:staged": "eslint \"**/*.ts\" --fix && pretty-quick --staged --verbose --pattern \"**/*.{ts}\"  --ignore-path \"./.eslintignore\" ",
        "format": "eslint \"**/*.ts\" --fix && npm run prettier"
    },
    "gitHooks": {
        "pre-commit": "npm run format:staged && npm run prettier"
    },
    "author": "Totto",
    "license": "MIT",
    "dependencies": {
        "brfs": "^2.0.2",
        "browserify": "^17.0.0",
        "chart.js": "^4.2.1",
        "esmify": "^2.1.1",
        "jquery": "^3.6.4",
        "mousetrap": "^1.6.5",
        "nodemon": "^2.0.22",
        "nodemw": "^0.19.0",
        "socket.io": "^4.6.1",
        "socket.io-client": "^4.6.1",
        "tsify": "^5.0.4",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.51",
        "@types/browserify": "^12.0.37",
        "@types/jest": "^29.5.0",
        "@types/mousetrap": "^1.6.11",
        "@types/node": "^18.15.11",
        "babel-preset-es2020": "^1.0.2",
        "babelify": "^10.0.0",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-deprecation": "^1.4.1",
        "eslint-plugin-github": "^4.7.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-regexp": "^1.14.0",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-userscripts": "^0.2.12",
        "prettier": "^2.8.7",
        "transform-loader": "^0.2.4",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tsconfig-to-swcconfig": "^2.2.0",
        "typescript": "^5.0.3"
    },
    "engines": {
        "node": ">=18.2.0",
        "npm": ">=8.10.0"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.7",
        "utf-8-validate": "^6.0.3"
    }
}
